#include <iostream>
using namespace std;

int main() {
  int n,i,arr[1020], aux;
  bool saFacutSchimbare = true;
  // Eroare
  cin>>n;
  // citirea array
  for(i=0;i<n;i++){
    cin>>arr[i];
  }
  cout<<"Array-ul introdus este: ";
  for(i=0;i<n;i++){
    cout<<arr[i]<<" ";
  } 
  cout<<endl;
  // n=3;
  // int arr[3]={6,2,8};
  // bubleshort
  // daca sa facut o schimbare ultima verificare
  while(saFacutSchimbare==true){
    saFacutSchimbare=false;
    // verificam daca puteam face o schimbare
    for(i=0;i<n-1;i++){
      if(arr[i]>arr[i+1]){
        aux=arr[i];
        arr[i]=arr[i+1];
        arr[i+1]=aux;
        saFacutSchimbare=true;
      }
    }
  }
  cout<<"Array-ul ordonat este: ";
  for(i=0;i<n;i++){
    cout<< arr[i]<<" ";
  } 
  // Time Complexity: O(n^2)=O(n*nr de schimbari)
  // Size Complexity: O(n)

  return 0;
}
